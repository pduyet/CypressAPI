{
  "scripts": {
    "cy-open": "cypress open",
    "cy-open-dev": "cypress run --reporter mochawesome --env type=dev",
    "cy-open-staging": "cypress run --reporter mochawesome --env type=staging",
    "mocha-merge": "npx mochawesome-merge ./cypress/reports/*.json -o mochawesome.json",
    "mocha-export": "npx marge mochawesome.json",
    "mocha-show": "cmd /c start mochawesome-report/mochawesome.html"
  },
  "dependencies": {
    "cypress": "^12.7.0",
    "marge": "^1.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.9.5"
  }
}
